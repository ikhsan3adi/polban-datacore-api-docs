          "semester": {
            "type": "integer"
          },
          "ip": {
            "type": "number",
            "format": "float"
          }
        }
      },
      "AngkatanIp": {
        "type": "object",
        "properties": {
          "angkatan": {
            "type": "string"
          },
          "ip": {
            "type": "number",
            "format": "float"
          }
        }
