        "responses": {
          "200": {
            "description": "Data tren IP tertinggi (per angkatan atau per semester)",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "$ref": "#/components/schemas/AkademikTrenIpTertinggiAngkatan"
